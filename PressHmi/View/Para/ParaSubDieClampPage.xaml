        <Style x:Key="FontAwesome">
            <Setter Property="TextElement.FontFamily" Value="pack://application:,,,/PressHmi;component/View/FontAwesome/#FontAwesome" />
        </Style>


        <conv:ClampMoveStatusConverter x:Key="_cmsc"/>
        <conv:IOColorConverter x:Key="_iocc"/>
        <conv:ClampStatusStringConverter x:Key="_cssc"/>
        <conv:ClampRelaxInPositionStringConverter x:Key="_cripsc"/>
    </Page.Resources>

