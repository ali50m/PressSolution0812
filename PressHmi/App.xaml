<Application x:Class="PressHmi.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PressHmi"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Styles/Bootstrap/Bootstrap.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Color/dark.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Size/normal.xaml"/>

                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Styles/MainStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Styles/ParaStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Styles/MessageStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/PressHmi;component/View/Styles/MaintainCsdStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
